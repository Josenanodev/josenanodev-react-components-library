    "babel-loader": "^9.1.3",
    "chromatic": "^6.19.9",
    "copyfiles": "^2.4.1",
    "eslint-plugin-storybook": "^0.6.12",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
